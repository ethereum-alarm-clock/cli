  "dependencies": {
    "@ethereum-alarm-clock/timenode-core": "5.1.1",
    "bignumber.js": "5.0.0",
    "bluebird": "3.5.1",
    "clear": "0.1.0",
    "cli-table2": "0.2.0",
    "commander": "2.17.1",
    "console.table": "0.10.0",
    "dotenv": "5.0.1",
    "eac.js-lib": "2.4.0",
    "ethereumjs-util": "5.2.0",
    "keen-tracking": "4.0.2",
    "lokijs": "1.5.3",
    "ora": "3.0.0",
    "readline-sync": "1.4.9",
    "web3": "0.20.6",
    "web3-providers-ws": "1.0.0-beta.33",
    "websocket": "1.0.26"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "5.4.0",
    "eslint-config-airbnb-base": "13.1.0",
